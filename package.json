{
  "name": "vue-books",
  "version": "1.0.0",
  "description": "Nuxt.js + Vuetify.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": ["node_modules"],    
    "moduleFileExtensions": [
      "js",
      "vue",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "jest-vue-preprocessor"
    }
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.28.0",
    "axios": "^0.17.1",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint-plugin-jest": "^21.4.0",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.3.1",
    "nuxt": "^1.0.0-rc11",
    "push-dir": "^0.4.1",
    "vue-test-utils": "^1.0.0-beta.6",
    "vuetify": "^0.17.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
